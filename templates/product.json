        "86e126f2-732e-4320-83f1-e84662e5f3ec": {
          "type": "liquid",
          "settings": {
            "custom_liquid": "<a href=\"#\" id=\"openUploadModal\" class=\"btn btn--secondary\">Ladda upp bild som gäst<\/a>\n<div id=\"uploadModal\" class=\"modal\" style=\"display: none;\">\n  <input type=\"file\" id=\"imageUpload\" accept=\"image\/*\">\n<\/div>\n{% if template == 'product' %}\n  <script>\n    document.addEventListener(\"DOMContentLoaded\", function () {\n      var openModalLink = document.getElementById(\"openUploadModal\");\n      var uploadModal = document.getElementById(\"uploadModal\");\n      var imageUpload = document.getElementById(\"imageUpload\");\n\n      openModalLink.addEventListener(\"click\", function (e) {\n        e.preventDefault(); \/\/ Förhindra standardbeteendet för länken\n        imageUpload.click(); \/\/ Öppna filuppladdningsdialogen när länken klickas på\n      });\n\n      imageUpload.addEventListener(\"change\", function () {\n        var file = imageUpload.files[0];\n        if (file) {\n          var reader = new FileReader();\n          reader.onload = function (event) {\n            var imageData = event.target.result;\n            localStorage.setItem(\"uploadedImage\", imageData);\n            uploadModal.style.display = \"none\"; \/\/ Dölj modalen efter att bilden har laddats upp\n          };\n          reader.readAsDataURL(file);\n        }\n      });\n    });\n  <\/script>\n{% endif %}",
